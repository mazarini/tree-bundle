{
    "name": "mazarini/tree-bundle",
    "description": "Tree of bundle with demo",
    "type": "symfony-bundle",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": ">=8.1",
        "symfony/console": "6.*",
        "symfony/twig-bundle": "6.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "6.*",
        "symfony/css-selector": "6.*",
        "symfony/debug-bundle": "6.*",
        "symfony/dotenv": "6.*",
        "symfony/flex": "^2.2",
        "symfony/framework-bundle": "6.*",
        "symfony/maker-bundle": "^1.48",
        "symfony/monolog-bundle": "^3.0",
        "symfony/phpunit-bridge": "6.*",
        "symfony/runtime": "6.*",
        "symfony/stopwatch": "6.*",
        "symfony/web-profiler-bundle": "6.*",
        "symfony/yaml": "6.*"
    },
    "autoload": {
        "psr-4": {
            "Mazarini\\TreeBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mazarini\\TreeBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "mazarini",
            "email": "mazarini@protonmail.com"
        }
    ],
    "replace": {},
    "config": {
        "vendor-dir": "vendor",
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.2.*",
            "docker": false
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
